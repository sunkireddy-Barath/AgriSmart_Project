{
  "expo": {
    "name": "AgriSmart",
    "slug": "agrismart",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#22c55e",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.agrismart.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#22c55e"
      },
      "package": "com.agrismart.app",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    }
  }
}
